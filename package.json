{
  "name": "@ctrl/qbittorrent",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for qbittorrent using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/qbittorrent",
  "homepage": "https://qbittorrent.vercel.app",
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "typescript",
    "qbittorrent"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc",
    "build:docs": "typedoc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=./junit.xml"
  },
  "dependencies": {
    "@ctrl/magnet-link": "^3.1.1",
    "@ctrl/shared-torrent": "^4.1.1",
    "@ctrl/torrent-file": "^2.0.2",
    "@ctrl/url-join": "^2.0.2",
    "formdata-node": "^4.3.3",
    "got": "^12.1.0",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "3.4.6",
    "@sindresorhus/tsconfig": "3.0.1",
    "@types/node": "18.0.3",
    "@types/tough-cookie": "4.0.2",
    "c8": "7.11.3",
    "p-wait-for": "4.1.0",
    "typedoc": "0.23.7",
    "typescript": "4.7.4",
    "vitest": "0.18.0"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "engines": {
    "node": ">=14.16"
  }
}
